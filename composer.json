{
    "name": "beapp/doctrine-extensions",
    "description": "Common extensions classes for Doctrine",
    "type": "library",
    "license": "MIT",
    "version": "0.0.1",
    "keywords": [
        "doctrine",
        "extensions"
    ],
    "authors": [
        {
            "name": "Damien Villeneuve",
            "email": "d.villeneuve@beapp.fr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "doctrine/orm": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Beapp\\Doctrine\\Extension\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Beapp\\Doctrine\\Extension\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
